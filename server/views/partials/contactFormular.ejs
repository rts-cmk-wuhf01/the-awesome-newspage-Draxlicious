   <!-- ##### Contact Form Area Start ##### -->
   <div class="contact-area section-padding-0-80">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="contact-title">
                        <h2>Contact us</h2>
                    </div>
                   
                </div>
            </div>

            <div class="row">
                <div class="col-12 col-lg-8">
                 <% if( typeof return_message != 'undefined'){ %>
                    <h4><%= return_message %></h4>
                <% } %>
                    <div class="contact-form-area">
                        <form action="/contact" method="post">
                            <div class="row">
                                <div class="col-12 col-lg-6">
                                    <input type="text" class="form-control" id="name" name="name" placeholder="Name"
                                    value="<%= typeof values != 'undefined' ? values.name: '' %>">
                                </div>
                                <div class="col-12 col-lg-6">
                                    <input type="email" class="form-control" id="email" name="email" placeholder="E-mail"
                                    value="<%= typeof values != 'undefined' ? values.email: '' %>">
                                </div>
                                <div class="col-12">
                                    <input type="text" class="form-control" id="subject" name="subject" placeholder="Subject"
                                    value="<%= typeof values != 'undefined' ? values.subject: '' %>">
                                </div>
                                <div class="col-12">
                                    <textarea class="form-control" id="message" name="message" cols="30" rows="10" placeholder="Message"> <%= typeof values != 'undefined' ? values.message : '' %></textarea>
                                </div>
                                <div class="col-12 text-center">
                                    <button class="btn newspaper-btn mt-30 w-100" type="submit">Send Message</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>

                <div class="col-12 col-lg-4">
                    <!-- Single Contact Information -->
                    <% formularAdress.forEach(formular => {%>

                    
                    <div class="single-contact-information mb-30">
                        <h6><%= formular.title %>:</h6>
                        <p><%= formular.main %> <br><%= formular.second %></p>
                    </div>
                    <%})%>
                </div>
            </div>