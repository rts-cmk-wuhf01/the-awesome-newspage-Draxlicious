<!-- Single Featured Post -->
<% let now = new Date('2019-04-14 07:00:14'); %>


<% latestPost.forEach(post =>{ %>
    <div class="single-blog-post small-featured-post d-flex">
        <div class="post-thumb">
            <a href="#"><img src="<%= post.img %>" alt=""></a>
        </div>
        <div class="post-data">
            <a href="#" class="post-catagory"><%= post.name %></a>
            <div class="post-meta">
                <a href="#" class="post-title">
                    <h6><%= post.text %></h6>
                </a>
                <p class="post-date"><span><%= dateAndTime.format(now, 'h:mm A | MMMM DD') %> </span>
            </div>
        </div>
    </div>
<%})%>