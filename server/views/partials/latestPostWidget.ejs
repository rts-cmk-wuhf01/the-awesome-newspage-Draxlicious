<% latestPost.forEach(post =>{ %>

<div class="latest-posts-widget mb-50">
    <!-- Single Featured Post -->
    <div class="single-blog-post small-featured-post d-flex">
        <div class="post-thumb">
            <a href="#"><img src="<%= post.img%>" alt=""></a>
        </div>
        <div class="post-data">
            <a href="#" class="post-catagory"><%= post.name%></a>
            <div class="post-meta">
                <a href="#" class="post-title">
                    <h6><%= post.text %></h6>
                </a>
                <p class="post-date"><span><%= post.hour%> AM</span> | <span>April <%= post.date %></span></p>
            </div>
        </div>
    </div>


</div>    
<% }) %>