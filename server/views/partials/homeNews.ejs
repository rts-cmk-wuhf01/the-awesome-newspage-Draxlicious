                    <div class="row">
                    <% if(typeof homeNews != 'undefined'){ %> 
                        <% homeNews.forEach(news => {%>

                        <!-- Single Post -->
                        <div class="col-12 col-md-6">
                            <div class="single-blog-post style-3">
                                <div class="post-thumb">
                                    <a href="#"><img src="/img/bg-img/<%= news.image_name %>" alt=""></a>
                                </div>
                                <div class="post-data">
                                    <a href="#" class="post-catagory"><%= news.category_title%></a>
                                    <a href="#" class="post-title">
                                        <h6><%= news.article_content%></h6>
                                    </a>
                                    <div class="post-meta d-flex align-items-center">
                                        <a href="#" class="post-like"><img src="img/core-img/like.png" alt="">
                                            <span><%= news.article_likes%></span></a>
                                        <a href="#" class="post-comment"><img src="img/core-img/chat.png" alt="">
                                            <span><%= news.article_comment%></span></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <%})%>
                        <% } %>
                    </div>