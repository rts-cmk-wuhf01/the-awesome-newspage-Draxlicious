 <!-- ##### Team Area Start ##### -->
    <section class="newspaper-team mb-30">
        <div class="container">
            <div class="row">

                <!-- Single Team Member -->
                   
        <% aboutFromPictures.forEach(picture => { %>
            
                <div class="col-12 col-sm-6 col-lg-3">
                    <div class="single-team-member">
                        <img src="<%= picture.img %>" alt="">
                        <div class="team-info">
                            <h5><%= picture.name%></h5>
                            <h6><%= picture.job%></h6>
                        </div>
                    </div>
                </div>
        <%})%>
                
            </div>
        </div>
    </section>
    <!-- ##### Team Area End ##### -->